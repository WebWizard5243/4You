}

.negotiation-box button:hover {
  background-color: #48c0e0;
}

.response-message {
  margin-top: 15px;
  padding: 10px;
  border-radius: 6px;
  font-weight: 500;
  display: none;
}

.response-message.accepted {
  background-color: #d4edda;
  color: #155724;
}

.response-message.rejected {
